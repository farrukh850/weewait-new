{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/add-photo-daycare", "destination": "/login-register/add-photo-daycare.html" },
    { "source": "/about-daycare", "destination": "/login-register/about-daycare.html" },
    { "source": "/account-information", "destination": "/login-register/account-information.html" },
    { "source": "/signup-daycare", "destination": "/login-register/signup-daycare.html" },
    { "source": "/invitation-code", "destination": "/login-register/invitation-code.html" },
    { "source": "/confirm-email", "destination": "/login-register/confirm-email.html" },
    { "source": "/verification-code-error", "destination": "/login-register/verification-code-error.html" },
    { "source": "/verification-code", "destination": "/login-register/verification-code.html" },
    { "source": "/restore-password", "destination": "/login-register/restore-password.html" },
    { "source": "/login-error", "destination": "/login-register/login-error.html" },
    { "source": "/login", "destination": "/login-register/login.html" },
    { "source": "/pricing", "destination": "/pricing.html" },
    { "source": "/faq", "destination": "/faq.html" },
    { "source": "/how-it-works", "destination": "/how-it-works.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/thank-you", "destination": "/thank-you.html" },
    { "source": "/terms-daycare", "destination": "/terms-daycare.html" },
    { "source": "/terms-parents", "destination": "/terms-parents.html" },
    { "source": "/404", "destination": "/404.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/daycare-list-view", "destination": "/daycare-list-view.html" },
    { "source": "/daycare-map-view", "destination": "/daycare-map-view.html" },
    { "source": "/daycare-map-view-sidebar-hide", "destination": "/daycare-map-view-sidebar-hide.html" },
    { "source": "/:path*", "destination": "/index.html" }
  ]
}
