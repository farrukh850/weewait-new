{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/login", "destination": "/login.html" },
    { "source": "/signup", "destination": "/signup.html" },
    { "source": "/add-photo-daycare", "destination": "/add-photo-daycare.html" },
    { "source": "/about-daycare", "destination": "/about-daycare.html" },
    { "source": "/account-information", "destination": "/account-information.html" },
    { "source": "/signup-daycare", "destination": "/signup-daycare.html" },
    { "source": "/invitation-code", "destination": "/invitation-code.html" },
    { "source": "/confirm-email", "destination": "/confirm-email.html" },
    { "source": "/verification-code-error", "destination": "/verification-code-error.html" },
    { "source": "/verification-code", "destination": "/verification-code.html" },
    { "source": "/restore-password", "destination": "/restore-password.html" },
    { "source": "/login-error", "destination": "/login-error.html" },
    { "source": "/set-new-password", "destination": "/set-new-password.html" },
    { "source": "/pricing", "destination": "/pricing.html" },
    { "source": "/faq", "destination": "/faq.html" },
    { "source": "/how-it-works", "destination": "/how-it-works.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/thank-you", "destination": "/thank-you.html" },
    { "source": "/terms-daycare", "destination": "/terms-daycare.html" },
    { "source": "/terms-parents", "destination": "/terms-parents.html" },
    { "source": "/404", "destination": "/404.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/daycare-list-view", "destination": "/daycare-list-view.html" },
    { "source": "/daycare-map-view", "destination": "/daycare-map-view.html" },
    { "source": "/daycare-map-view-sidebar-hide", "destination": "/daycare-map-view-sidebar-hide.html" },
    { "source": "/(.*)", "destination": "/404.html" }
  ]
}
